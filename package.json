{
  "name": "hmpps-challenge-support-intervention-plan-ui",
  "version": "0.0.1",
  "description": "Hmpps Challenge Support Intervention Plan Ui",
  "repository": "git@github.com:ministryofjustice/hmpps-challenge-support-intervention-plan-ui.git",
  "license": "MIT",
  "scripts": {
    "prepare": "husky",
    "watch-ts": "tsc -w",
    "build": "node esbuild/esbuild.config.js --build",
    "start": "node $NODE_OPTIONS dist/server.js | bunyan -o short",
    "start:dev": "concurrently -k -p \"[{name}]\" -n \"ESBuild,Node\" -c \"yellow.bold,cyan.bold\" \"node esbuild/esbuild.config.js --build --watch\" \"node esbuild/esbuild.config.js --dev-server\"",
    "start-e2e": "export $(cat e2e-test.env) && node $NODE_DEBUG_OPTION dist/server.js | bunyan -o short",
    "start-e2e:watch": "export $(cat e2e-test.env) && concurrently -k -p \"[{name}]\" -n \"ESBuild,Node\" -c \"yellow.bold,cyan.bold\" \"node esbuild/esbuild.config.js --build --watch\" \"node esbuild/esbuild.config.js --dev-server\"",
    "lint": "eslint . --cache --max-warnings 0",
    "lint-fix": "eslint . --cache --max-warnings 0 --fix",
    "typecheck": "tsc",
    "test": "jest --detectOpenHandles --forceExit",
    "test:ci": "jest --detectOpenHandles --runInBand",
    "security_audit": "npx audit-ci --config audit-ci.json",
    "int-test": "cypress run --config video=false",
    "int-test-ui": "cypress open --e2e --browser chrome",
    "clean": "rm -rf dist node_modules",
    "rebuild": "npm run clean && npm i && npm run build"
  },
  "engines": {
    "node": "^20",
    "npm": "^10"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "server/**/*.{ts,js,jsx,mjs}"
    ],
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": "test_results/jest/"
        }
      ],
      [
        "./node_modules/jest-html-reporter",
        {
          "outputPath": "test_results/unit-test-reports.html"
        }
      ]
    ],
    "projects": [
      "./server",
      "./server/routes/journeys"
    ]
  },
  "lint-staged": {
    "*.{ts,js,css}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-sqs": "^3.637.0",
    "@microsoft/applicationinsights-clickanalytics-js": "^3.3.1",
    "@microsoft/applicationinsights-web": "^3.3.1",
    "@ministryofjustice/frontend": "^2.2.0",
    "@ministryofjustice/hmpps-connect-dps-components": "^1.2.0",
    "@opentelemetry/api": "~1.8.0",
    "@sentry/cli": "^2.33.1",
    "@sentry/node": "^8.26.0",
    "@types/express-serve-static-core": "^4.19.5",
    "agentkeepalive": "^4.5.0",
    "applicationinsights": "^2.9.5",
    "body-parser": "^1.20.2",
    "bunyan": "^1.8.15",
    "bunyan-format": "^0.2.1",
    "compression": "^1.7.4",
    "connect-flash": "^0.1.1",
    "connect-redis": "^7.1.1",
    "csurf": "^1.11.0",
    "date-fns": "^3.6.0",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "govuk-frontend": "^5.5.0",
    "helmet": "^7.1.0",
    "http-errors": "^2.0.0",
    "jwt-decode": "^4.0.0",
    "nocache": "^4.0.0",
    "nunjucks": "^3.2.4",
    "openapi-typescript": "^7.3.0",
    "passport": "^0.7.0",
    "passport-oauth2": "^1.8.0",
    "redis": "^4.7.0",
    "superagent": "^10.0.2",
    "url-value-parser": "^2.2.0",
    "uuid": "^10.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@jgoz/esbuild-plugin-typecheck": "^4.0.1",
    "@testing-library/cypress": "^10.0.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/user-event": "^14.5.2",
    "@types/bunyan": "^1.8.11",
    "@types/bunyan-format": "^0.2.9",
    "@types/compression": "^1.7.5",
    "@types/connect-flash": "0.0.40",
    "@types/csurf": "^1.11.5",
    "@types/express-session": "^1.18.0",
    "@types/http-errors": "^2.0.4",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.16.1",
    "@types/nunjucks": "^3.2.6",
    "@types/passport": "^1.0.16",
    "@types/passport-oauth2": "^1.4.17",
    "@types/superagent": "^8.1.8",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "audit-ci": "^7.1.0",
    "aws-sdk-client-mock": "^4.0.1",
    "axe-core": "^4.10.0",
    "chai": "^5.1.1",
    "chokidar": "^3.6.0",
    "concurrently": "^8.2.2",
    "cypress": "^13.13.3",
    "cypress-axe": "^1.5.0",
    "cypress-multi-reporters": "^1.6.4",
    "dotenv": "^16.4.5",
    "esbuild": "^0.23.1",
    "esbuild-plugin-clean": "^1.0.1",
    "esbuild-plugin-copy": "^2.1.1",
    "esbuild-plugin-manifest": "^1.0.4",
    "esbuild-sass-plugin": "^3.3.1",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-cypress": "^3.5.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "eslint-plugin-prettier": "^5.2.1",
    "glob": "^11.0.0",
    "husky": "^9.1.5",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fixed-jsdom": "^0.0.2",
    "jest-html-reporter": "^3.10.2",
    "jest-junit": "^16.0.0",
    "jsonwebtoken": "^9.0.2",
    "lint-staged": "^15.2.9",
    "mocha-junit-reporter": "^2.2.1",
    "nock": "^13.5.5",
    "prettier": "^3.3.3",
    "prettier-plugin-jinja-template": "^1.4.1",
    "sass": "^1.77.8",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.4",
    "typescript": "^5.5.4"
  }
}
